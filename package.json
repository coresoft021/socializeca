{
  "name": "angular2-express-starter",
  "version": "1.0.0",
  "description": "Starter application of Angular2 with Express",
  "main": "server/app.js",
  "license": "MIT",
  "scripts": {
    "startx": "concurrently --kill-others \"npm run _server:run\" \"ng serve --aot --progress=false --proxy-config proxy.conf.json\"",
     "start": "concurrently --kill-others \"npm run _server:run\" \"http-server -p 8080 ./dist/core/public/\"",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "test:client": "ng test",
    "e2e:client": "ng e2e",
    "build": "npm run _server:build",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./server",
    "postinstall": "npm run build"
  },
  "author": "Vlado Tešanović",
  "repository": {
    "url": "https://github.com/vladotesanovic/angular2-express-starter"
  },
  "private": true,
  "dependencies": {

     "async": "2.4.0",
     "bootstrap": "3.3.7",
     "pg": "7.4.0",
    "http-server": "^0.11.1",
    "pg-hstore": "2.3.2",
    "pg-native": "1.10.0",    
    "serve-favicon": "2.3.0",
    "validator": "7.0.0",    
    "sequelize": "4.22.6",
    "sequelize-cli": "4.0.0",
    "mysqldump": "^2.2.1",
    "body-parser": "~1.17.1",
    "compression": "1.6.2",
    "cookie-parser": "~1.3.5",
    "core-js": "^2.5.1",
     "cors": "^2.8.4",
     "jquery": "^3.2.1",
     "list.js": "^1.5.0",
     
    "express": "^4.15.2",
    "express-jwt": "^5.1.0",
    "jsonwebtoken": "^7.3.0",
    "ngrx-store-freeze": "^0.1.9",
    "normalize.css": "^5.0.0",
    "path": "^0.12.7",
    "rxjs": "^5.4.2",
    "number2text": "^1.1.0",
    "ts-helpers": "^1.1.2",
    "ts-node": "~3.3.0",
    "tslint": "^4.5.1",
    "typescript": "^2.5.3",
    "uuid": "^3.0.1",
     "underscore": "^1.8.3",
    "zone.js": "^0.8.18"
    
  },
  "devDependencies": {
    "codelyzer": "~3.1.2",
    "concurrently": "^3.5.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.5.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.2.1",
    "nodemon": "1.17.3",
    "protractor": "~5.1.0",
    "webdriver-manager": "10.2.5"
  }
}
